<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.kundeController.NyttKjopController">
   <children>
      <TreeTableView fx:id="tabell" layoutX="53.0" layoutY="50.0" prefHeight="464.0" prefWidth="412.0">
        <columns>
            <TreeTableColumn fx:id="coVaremerke" prefWidth="134.0" text="Varemerke">
            <cellValueFactory>
                <PropertyValueFactory property="Varemerke" />
            </cellValueFactory>
        </TreeTableColumn>
            <TreeTableColumn fx:id="coModell" prefWidth="130.5" text="Modell" />
            <TreeTableColumn fx:id="coPris" prefWidth="146.5" text="Pris" />
        </columns>
      </TreeTableView>
      <Button layoutX="488.0" layoutY="50.0" mnemonicParsing="false" onAction="#leggTilIHandekurv" prefHeight="49.0" prefWidth="211.0" text="Legg valgt vare i handekurv">
         <font>
            <Font size="15.0" />
         </font></Button>



      <TableView fx:id="tableHandekurv" layoutX="766.0" layoutY="50.0" prefHeight="228.0" prefWidth="211.0">
        <columns>
            <TableColumn prefWidth="75.0" text="Varenavn">
                <cellValueFactory>
                    <PropertyValueFactory property="Navn" />
                </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="coHandlekurvPris" prefWidth="75.0" text="Pris (kr)">
                <cellValueFactory>
                    <PropertyValueFactory property="Pris" />
                </cellValueFactory>
            </TableColumn>
        </columns>
      </TableView>
   </children>
</AnchorPane>
