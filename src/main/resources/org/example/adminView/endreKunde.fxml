<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" styleClass="endreKomponentBakgrunn" stylesheets="@../../../style/style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.adminController.EndreKunde">



      <Button layoutX="20.0" layoutY="561.0" mnemonicParsing="false" onAction="#tilbakeAction" text="Tilbake" />

      <TableView fx:id="tableView" editable="true" layoutX="20.0" layoutY="167.0" prefHeight="374.0" prefWidth="960.0">
         <columns>

         <TableColumn fx:id="coKundenummer"  prefWidth="115.0" text="Kundenummer">
             <cellValueFactory>
                 <PropertyValueFactory property="Kundenummer" />
             </cellValueFactory>
         </TableColumn>

         <TableColumn fx:id="coFornavn" editable="true" onEditCommit="#FornavnEdit" prefWidth="115.0" text="Fornavn">
             <cellFactory>
                 <TextFieldTableCell fx:factory="forTableColumn" />
             </cellFactory>
         </TableColumn>

         <TableColumn fx:id="coEtternavn" onEditCommit="#EtternavnEdit" prefWidth="115.0" text="Etternavn">
             <cellFactory>
                 <TextFieldTableCell fx:factory="forTableColumn" />
             </cellFactory>
         </TableColumn>

         <TableColumn fx:id="coBrukernavn" onEditCommit="#BrukernavnEdit" prefWidth="115.0" text="Brukernavn">
            <cellFactory>
                <TextFieldTableCell fx:factory="forTableColumn" />
            </cellFactory>
         </TableColumn>
         <TableColumn fx:id="coEpost" onEditCommit="#EpostEdit" prefWidth="160.0" text="Epost">
             <cellFactory>
                 <TextFieldTableCell fx:factory="forTableColumn" />
             </cellFactory>
         </TableColumn>
         <TableColumn fx:id="coPassord" onEditCommit="#PassordEdit" prefWidth="115.0" text="Passord">
             <cellValueFactory>
                 <PropertyValueFactory property="Passord" />
             </cellValueFactory>
             <cellFactory>
                 <TextFieldTableCell fx:factory="forTableColumn" />
             </cellFactory>
         </TableColumn>
         </columns>

      </TableView>
   <Label layoutX="20.0" layoutY="21.0" prefHeight="52.0" prefWidth="360.0" text="Endre kunder" textFill="WHITE">
      <font>
         <Font name="System Bold" size="26.0" />
      </font>
   </Label>

   <Label layoutX="20.0" layoutY="146.0" text="Dobbeltklikk på en celle for å gjør endringer.">
      <font>
         <Font name="System Bold" size="12.0" />
      </font>
   </Label>



</AnchorPane>
